{
    "version": 2,
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "nextjs",
    "build": {
        "env": {
            "NEXT_TELEMETRY_DISABLED": "1"
        }
    },
    "regions": [
        "iad1"
    ],
    "functions": {
        "src/app/api/**/*.ts": {
            "maxDuration": 30
        }
    }
}